<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mynavigation"
    app:startDestination="@id/dashboard">

    <fragment
        android:id="@+id/dashboard"
        android:name="com.criterion.nativevitalio.UI.fragments.Dashboard"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboard_to_drawer"
            app:destination="@id/drawer" />
    </fragment>
    <activity
        android:id="@+id/drawer"
        android:name="com.criterion.nativevitalio.UI.drawer"
        android:label="drawer" />
</navigation>